
CREATE TABLE JUNK_TEST(
    ID VARCHAR(4),
    DESCRIPTIONS VARCHAR(20)
);

INSERT INTO JUNK_TEST VALUES(1,'Employee12');
INSERT INTO JUNK_TEST VALUES(2,'Dept_#451');
INSERT INTO JUNK_TEST VALUES (5,'##@!!');
INSERT INTO JUNK_TEST VALUES(6,'HEY_HELLO');

CREATE OR REPLACE FUNCTION DETECT_JUNK(
    P_TEXT IN VARCHAR2
)
RETURN VARCHAR2
IS
V_RESULT VARCHAR(50);
BEGIN
    IF REGEXP_LIKE(P_TEXT,'[^A-Za-z0-9]') THEN
        V_RESULT:='JUNK';
    ELSE
        V_RESULT :='CLEAN DATA';
    END IF; 
    RETURN V_RESULT;
END;
/
SELECT DETECT_JUNK('##@89') AS RESULT FROM DUAL;

SELECT ID,DETECT_JUNK(DESCRIPTIONS) AS JUNK_STATUS
FROM JUNK_TEST;


CREATE OR REPLACE FUNCTION REMOVE_JUNK(
    P_TEXT IN VARCHAR2
)
RETURN VARCHAR2
IS
V_CLEAN VARCHAR2(100);
BEGIN
V_CLEAN:=REGEXP_REPLACE(P_TEXT,'[^A-Za-z0-9 _]','');
return V_CLEAN;
END;
/

SELECT ID,REMOVE_JUNK(DESCRIPTIONS) AS CLEANED_TEXT
FROM JUNK_TEST;


CREATE OR REPLACE PROCEDURE POWER_NUM(
    NUM1 IN NUMBER,
    NUM2 IN NUMBER
)IS 
POWER_RESULT NUMBER;
BEGIN
    POWER_RESULT:= POWER(NUM1,NUM2);
    DBMS_OUTPUT.PUT_LINE('POWER OF NUMBER IS: ' || POWER_RESULT);

END POWER_NUM;


BEGIN
    POWER_NUM(10,2);
END;
/
SELECT * FROM EMPLOYEES;
