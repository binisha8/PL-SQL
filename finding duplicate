CREATE TABLE student10 (
    student_id NUMBER,
    student_name VARCHAR2(50)
);
INSERT INTO student10 VALUES (1, 'Amit');
INSERT INTO student10 VALUES (2, 'Riya');
INSERT INTO student10 VALUES (3, 'Amit');   -- duplicate name
INSERT INTO student10 VALUES (4, 'John');
INSERT INTO student10 VALUES (5, 'Riya');   -- duplicate name


SELECT student_name, COUNT(*)
FROM students
GROUP BY student_name
HAVING COUNT(*) > 1;

SELECT student_id, student_name, COUNT(*)
FROM students
GROUP BY student_id, student_name
HAVING COUNT(*) > 1;


delete from student10 s
where rowid not in(select min(rowid)
                   from student10
                   group by student_name);
--ROWID is a special, hidden column in Oracle that uniquely identifies every row in a table
select * from student10;

